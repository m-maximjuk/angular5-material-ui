{
  "name": "@bleenco/angular-webpack-seed",
  "version": "0.5.0",
  "scripts": {
    "start": "webpack-dev-server --env.client --env.dev --env.serve --progress --hot",
    "prebuild:client": "npm run clean",
    "build:client": "webpack --progress",
    "prebuild:client:prod": "npm run clean",
    "build:client:prod": "webpack -p --progress",
    "build:ssr": "ngc -p ./src/tsconfig.server.json && webpack --config ./webpack.config.server.js --progress && rimraf ./lib",
    "build": "npm run build:client && npm run build:ssr",
    "build:prod": "npm run build:client:prod && npm run build:ssr",
    "prebuild": "npm run clean",
    "prebuild:prod": "npm run clean",
    "clean": "rimraf dist",
    "server": "node dist/server.js",
    "dev": "webpack --env.server --progress --hide-modules --watch & webpack-dev-server --env.client --env.serve --progress",
    "dev:aot": "webpack --env.aot --env.server --progress --hide-modules --watch & webpack-dev-server --env.aot --env.client --env.serve --progress",
    "watch": "nodemon ./dist/server.js",
    "test": "karma start",
    "e2e:live": "npm run e2e -- --elementExplorer",
    "e2e": "protractor ./protractor.conf.js",
    "pree2e": "npm run webdriver:update -- --standalone",
    "webdriver-manager": "webdriver-manager",
    "webdriver:start": "npm run webdriver-manager start",
    "webdriver:update": "npm run webdriver-manager update"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bleenco/angular-webpack-seed.git"
  },
  "authors": [
    "Jan Kuri <jkuri88@gmail.com>",
    "Irman Abdic <irman.abdic@gmail.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bleenco/angular-webpack-seed/issues"
  },
  "homepage": "https://github.com/bleenco/angular-webpack-seed#readme",
  "dependencies": {
    "@angular/animations": "^5.0.1",
    "@angular/common": "^5.0.1",
    "@angular/compiler": "^5.0.1",
    "@angular/core": "^5.0.1",
    "@angular/forms": "^5.0.1",
    "@angular/http": "^5.0.1",
    "@angular/platform-browser": "^5.0.1",
    "@angular/platform-browser-dynamic": "^5.0.1",
    "@angular/platform-server": "^5.0.1",
    "@angular/router": "^5.0.1",
    "core-js": "^2.5.1",
    "express": "^4.16.2",
    "rxjs": "^5.5.2",
    "xhr2": "^0.1.4",
    "zone.js": "~0.8.18"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^5.0.1",
    "@ngtools/webpack": "^1.8.0",
    "@nguniversal/module-map-ngfactory-loader": "^5.0.0-beta.5",
    "@types/express": "^4.0.39",
    "@types/jasmine": "^2.6.3",
    "@types/node": "^8.0.51",
    "codelyzer": "^4.0.1",
    "compression-webpack-plugin": "^1.0.1",
    "copy-webpack-plugin": "^4.2.0",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.30.1",
    "jasmine": "^2.8.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^1.7.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-jasmine": "^1.1.0",
    "karma-spec-reporter": "^0.0.31",
    "karma-webpack": "^2.0.6",
    "ng-router-loader": "^2.1.0",
    "node-sass": "^4.6.0",
    "portfinder": "^1.0.13",
    "protractor": "^5.2.0",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "script-ext-html-webpack-plugin": "^1.8.8",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.19.0",
    "to-string-loader": "^1.1.5",
    "ts-loader": "^3.1.1",
    "ts-node": "^3.3.0",
    "tslint": "^5.8.0",
    "typescript": "^2.6.1",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4",
    "webpack-dll-bundles-plugin": "^1.0.0-beta.5",
    "webpack-merge": "^4.1.1",
    "webpack-node-externals": "^1.6.0"
  }
}
